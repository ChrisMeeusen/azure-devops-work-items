{
  "name": "azure-devops-work-items",
  "productName": "azure-devops-work-items",
  "release": "never",
  "version": "0.0.1-rc2",
  "main": "public/electron.js",
  "description": "This application provides a git hook to query your teams devops work items.",
  "author": "Chris Meeusen",
  "private": false,
  "homepage": "./",
  "bin": {
    "ado-hook": "hook-installer.js"
  },
  "scripts": {
    "postinstall": "node post-install.js",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "pkg-sync": "node package-script.js",
    "pkg-rename": "node package-rename.js",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "electron-build:all": "electron-builder -mwl",
    "build": "npm run react-build && npm run electron-build",
    "build:all": "npm run react-build && npm run electron-build:all",
    "build:release": "env NODE_ENV=production npm run build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.35",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "@types/toastr": "^2.1.37",
    "cross-env": "^7.0.2",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-react-devtools": "^0.5.3",
    "foundation-sites": "^6.6.3",
    "jquery": "^3.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "rxjs": "^6.5.5",
    "toastr": "^2.1.4",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/react-redux": "^7.1.7",
    "concurrently": "^5.1.0",
    "electron": "^8.2.2",
    "electron-builder": "^22.5.1",
    "googleapis": "^39.2.0",
    "node-sass": "^4.13.1",
    "wait-on": "^4.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
